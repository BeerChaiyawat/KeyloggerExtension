{
  "manifest_version": 3,
  "name": "Keylogger Example",
  "version": "1.0",
  "permissions": ["activeTab", "scripting","http://localhost/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
